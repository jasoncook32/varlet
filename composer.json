{
    "name": "riipandi/valet-windows",
    "description": "Yet another fork of Laravel Valet for Windows.",
    "keywords": ["laravel", "valet", "windows"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Adam Wathan",
            "email": "adam.wathan@gmail.com"
        },
        {
            "name": "Cretu Eusebiu",
            "email": "me@cretueusebiu.com"
        },
        {
            "name": "Aris Ripandi",
            "email": "aris@ripandi.id"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/container": "~5.1 | ^6.0",
        "mnapoli/silly": "~1.0",
        "nategood/httpful": "~0.2",
        "phpseclib/phpseclib": "^2.0",
        "symfony/process": "~2.7|~3.0|~4.0",
        "tightenco/collect": "^5.3 | ^6.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "files": [
            "cli/includes/compatibility.php",
            "cli/includes/facades.php",
            "cli/includes/helpers.php"
        ],
        "psr-4": {
            "Valet\\": "cli/Valet/"
        }
    },
    "bin": [
        "valet"
    ]
}
